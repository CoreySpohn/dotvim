{
  "suggest.enablePreview": true,
  "suggest.minTriggerInputLength": 3,
  "suggest.removeDuplicateItems": true,
  "suggest.snippetIndicator": "[+] ",
  "suggest.timeout": 1000,
  "coc.source.ultisnips.enable": true,
  "coc.source.omni.filetypes": ["foam"],
  "python.linting.maxNumberOfProblems": 1000,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintUseMinimalCheckers": false,
  "pyright.disableDiagnostics": true,
  "python.trace.server": "messages",
  "python.analysis.diagnosticEnabled": false,
  "coc.preferences.useQuickfixForLocations": true,
  "snippets.ultisnips.directories": ["~/.vim/UltiSnips/"]
}
